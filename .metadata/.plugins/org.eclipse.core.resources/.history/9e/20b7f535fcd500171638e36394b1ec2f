
public class UsingVariables 
{

	public static void main(String[] args)
	{
	
	int room;
	double value_of_e;
	String class_name;

	room = 113;
	value_of_e = 2.718282;
	class_name = CompSci;
	
	System.out.println("The room number is" + room + ".");
	System.out.println("The value of e is about" + value_of_e + ".");
	System.out.println("I am learning a bit about" + class_name + ".");
	}
}
